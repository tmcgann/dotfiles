# Add `~/bin` to the `$PATH`
export PATH="$HOME/bin:$PATH";

# Add `/usr/local/sbin` to the `$PATH`
export PATH="$PATH:/usr/local/sbin";

# Add Yarn binaries to the `$PATH`
export PATH="$PATH:`yarn global bin`";

# Favor Brew's OpenSSL over Apple's
export PATH="/usr/local/opt/openssl/bin:$PATH"

# Add Python2 to PATH
export PATH="/usr/local/opt/python@2/libexec/bin:$PATH"

# Add the Python user install directory (i.e. Python packages installed via pip with the `--user` flag)
export PATH="$PATH:/Users/taylor/Library/Python/2.7/bin"
